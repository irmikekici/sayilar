#include <stdio.h>
#include <stdlib.h>

void yerdegistir(int x,int y)
{
	int gecici;
	printf("Yer degistirilmeden onceki hali= %d  %d\n",x,y);
	gecici=x;
	x=y;
	y=gecici;
	printf("Yer degistirdikten sonraki hali= %d  %d\n",x,y);
	
}
int main()
{
	int a,b;
	printf("Lutfen iki adet tam sayi girniz\n");
	scanf("%d%d",&a,&b);
	yerdegistir(a,b);
	
	return 0;
}
